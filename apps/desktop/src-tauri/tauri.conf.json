{
  "productName": "HARDY - HAR viewer",
  "version": "0.1.0",
  "identifier": "com.Peter-Gal.dev",
  "build": {
    "frontendDist": "../../web/out",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "HARDY | HAR viewer",
        "width": 1600,
        "minWidth": 750,
        "height": 900,
        "minHeight": 450,
        "resizable": true,
        "maximized": true,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "window-minimize",
          "description": "This capability allows the `main` window to be minimized.",
          "windows": ["main"],
          "platforms": ["linux", "macOS", "windows"],
          "permissions": [
            "core:window:allow-minimize",
            "core:window:allow-toggle-maximize",
            "core:window:allow-close"
          ]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../../../node_modules/@tauri-apps/cli/schema.json"
}
